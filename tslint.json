{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "curly": false,
        "max-classes-per-file": [false],
        "no-angle-bracket-type-assertion": false,
        "interface-name": false,
        "no-bitwise": false,
        "no-console": false,
        "quotemark": false,
        "whitespace": false,
        "variable-name": false,
        "class-name": false,
        "max-line-length": false
    },
    "rulesDirectory": []
}